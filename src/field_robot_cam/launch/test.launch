<launch>
  <node pkg="rosserial_python" type="serial_node.py" name="stm32_serial">
    <param name="port" value="/dev/ttyUSB1"/>
    <param name="baud" value="115200"/>
  </node>

  <node pkg="field_robot_cam" type="vision_follower.py" name="vision_follower"
        output="screen" launch-prefix="python3">
  </node>
  

  <node pkg="field_robot_cam" type="mode_manager.py" name="mode_manager"
        output="screen">
  </node>
</launch>

